[alias]
	st = status -s
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --branches
	lo = log --oneline --graph --all
[credential]
	helper = store
[help]
	autocorrect = 1
[init]
	defaultBranch = main
[merge]
	conflictstyle = diff3
[push]
	followTags = true
	autoSetupRemote = true
